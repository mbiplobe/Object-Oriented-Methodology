package inheritance;

public class Student{
	private String name;
	private String id;
	private String mobile;
	
	
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public void setMobile(String name) {
		this.name = name;
	}
	public String getId() {
		return id;
	}
	public void setId(String id) {
		this.id = id;
	}
	public String getMobile() {
		return mobile;
	}
	
	public void showDetails(){
		String details="Student Name :"+getName()+"\nStudent Id :"+getId()+"\nStudent Mobile :"+getMobile();
	}
}